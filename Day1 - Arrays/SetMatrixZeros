class Solution {
public:
    int maxProfit(vector<int>& p) 
    {
        int n = p.size();
        int profit=0,minm=p[0];
        for(int i=1;i<n;i++)
        {
            int cost = p[i] - minm;
            profit = max(profit,cost);
            minm = min(minm,p[i]);
        }
        return profit;
    }
};
