class Solution {
public:
    void rotate(vector<vector<int>>& matrix) 
    {
        int m = matrix.size();
        vector<int>v(m*m);
        for(int i=0;i<m;i++)
        {
            for(int j=0;j<m;j++)
            {
                v[j*m+(m-i-1)] = matrix[i][j];
            }
        }
        int k=0;
        for(int i=0;i<m;i++)
        {
            for(int j=0;j<m;j++)
            {
                matrix[i][j]= v[k];
                k++;
            }
        }
    }
};